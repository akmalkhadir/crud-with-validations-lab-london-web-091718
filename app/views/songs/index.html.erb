<h1>Songs List</h1>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th></th>
    </tr>
  </thead>
<tbody>
  <% @songs.each do |song| %>
  <tr>
    <td> <%= link_to song.title, song_path(song) %></td>
    <td> <%= link_to 'Delete', song, method: :delete, data: { confirm: 'Sure Yo??' } %> </td>
  </tr>
  <% end %>
</tbody>
</table>
